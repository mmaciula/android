        </activity>
        <activity android:name=".NewClass" />
        <activity android:name=".IntClass" />
        <activity android:name=".DoubleClass" />
        <activity android:name=".Boolean" />
        <activity android:name=".StringType"></activity>
    </application>

</manifest>